{
  "family": "linc-detector-api-task-definition",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": 2048,
  "memory": 4096,
  "executionRoleArn": "arn:aws:iam::134918431080:user/linc-detector-api",
  "containerDefinitions": [
    {
      "name": "linc-detector-api-container",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ]
}
